   "source": [
    "def solution(S, A):\n",
    "    alfa = S[0]\n",
    "    i = A[0]\n",
    "    while i != 0:\n",
    "        alfa += S[i]\n",
    "        i = A[i]\n",
    "    return alfa"
   ]
